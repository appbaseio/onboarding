  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack-server": "webpack-dev-server --hot --inline --progress --colors --port 8000",
    "web-server": "http-server -p 3000 .",
    "start": "concurrently \"npm run webpack-server\" \"gulp watch\"",
